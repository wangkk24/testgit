<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:focusable="true"
    android:layout_marginLeft="@dimen/margin_7.5"
    android:layout_marginRight="@dimen/margin_7.5"
    android:layout_height="wrap_content">
    <ImageView
        android:id="@+id/mvtv_filter_select"
        android:src="@drawable/p_select"
        android:visibility="gone"
        android:layout_marginLeft="@dimen/margin_18p7"
        android:layout_centerVertical="true"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />
    <TextView
        android:id="@+id/mytv_filter_view"
        android:gravity="center_vertical"
        android:layout_marginLeft="@dimen/margin_36"
        android:textSize="@dimen/order_title_text_size"
        android:textColor="@color/c21_color"
        android:layout_width="match_parent"
        android:layout_height="@dimen/margin_36" />

</RelativeLayout>