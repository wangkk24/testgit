        android:id="@+id/rl_rightItem_container02"
        app:posterType="1"
        android:layout_width="@dimen/margin_231.3"
        android:layout_height="@dimen/margin_126.67"
        android:layout_marginLeft="@dimen/pan_common_item_marginLeft"
        android:layout_toEndOf="@+id/rl_mix_item1_parent"
        android:layout_below="@+id/rl_rightItem_container01"
        android:layout_marginTop="@dimen/margin_16"
