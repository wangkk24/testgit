    <declare-styleable name="CustomTVRecyclerView">
        <attr name="spanCount" format="integer"/>
        <attr name="layout_style" format="enum">
            <enum name="horizontal" value="0"/>
            <enum name="vertical" value="1"/>
            <enum name="grid_vertical" value="2"/>
            <enum name="grid_horizontal" value="3"/>
            <enum name="other" value="9"/>
        </attr>
        <attr name="autoMeasureEnable" format="boolean"/>
    </declare-styleable>


    <!--TV控件-->
    <!-- 倒影控件属性 -->
    <declare-styleable name="reflectItemView">
        <attr name="isReflect"  format="boolean" />
        <attr name="isShape"  format="boolean" />
        <attr name="reflect_height"  format="dimension" />
        <attr name="radius"  format="dimension" />
