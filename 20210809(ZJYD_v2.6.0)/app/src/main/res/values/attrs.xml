    <declare-styleable name="RelativeLayoutExt">
        <attr name="bgRLayoutNinePatch" format="reference" />
        <attr name="bgRLayout" format="reference" />
        <attr name="bgRLayoutNormal" format="reference" />
        <attr name="bgRLayoutSelected" format="reference" />
        <attr name="bgRLayoutColor" format="reference" />
        <attr name="bgRLayoutColorNormal" format="reference" />
        <attr name="bgRLayoutColorSelected" format="reference" />
        <attr name="bgRLayoutColorFocused" format="reference" />
    </declare-styleable>
    <declare-styleable name="LinearLayoutExt">
        <attr name="bgLLayoutNinePatch" format="reference" />
        <attr name="bgLLayout" format="reference" />
        <attr name="bgLLayoutNormal" format="reference" />
        <attr name="bgLLayoutSelected" format="reference" />
        <attr name="bgLLayoutColor" format="reference" />
        <attr name="bgLLayoutColorNormal" format="reference" />
        <attr name="bgLLayoutColorSelected" format="reference" />
        <attr name="bgLLayoutColorFocused" format="reference" />
