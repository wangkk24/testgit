        <attr name="colorTxtSelected" format="reference" />
    </declare-styleable>
    <declare-styleable name="EditTextExt">
        <attr name="bgEditNinePatch" format="reference" />
        <attr name="bgEdit" format="reference" />
        <attr name="bgColorEdit" format="reference" />
        <attr name="colorEdit" format="reference" />
        <attr name="colorHintEdit" format="reference" />
        <attr name="textHintEdit" format="reference" />
